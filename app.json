{
  "expo": {
    "name": "Cezs",
    "slug": "Cezs",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/cezslogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#2e2e2e"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mmmert.cezs"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a7c41134-0bcd-4818-986b-ffbe5b434321"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ec4b00b3-48b9-4d4f-a22c-2b8c326ab1cb"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow access to upload your avatar or video"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow access to record yourself playing your instrument"
        }
      ]
    ]
  }
}
